{
  "crons": [
    {
      "path": "/api/cronjobs",
      "schedule": "0 45 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(.*)\\.vercel\\.app"
        }
      ],
      "destination": "https://sealdrop.xyz/:path*",
      "permanent": true
    }
  ]
}